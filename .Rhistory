# Duas regressões lineares com controles
regs_lineares_controles <- lm_robust(valor_empenhado_ipca ~ vitoria_pp*votos_razao_pp_centr + regiao + pop + PIBpc + taxa_escolarizacao + mortalidade_infantil, data = dt_todas)
# Duas regressões com linearidade flexibilizada e controles
regs_flexiveis_controles <- lm_robust(valor_empenhado_ipca ~ vitoria_pp*votos_razao_pp_centr + vitoria_pp*votos_razao_pp_centr_sq + regiao + pop + PIBpc + taxa_escolarizacao + mortalidade_infantil, data = dt_todas)
# Resultados
modelsummary(list(
"Duas regressões lineares" = regs_lineares,
"Duas regressões não lineares" = regs_flexiveis,
"Duas regressões lineares com controles" = regs_lineares_controles,
"Duas regressões não lineares com controles" = regs_flexiveis_controles
),
statistic = "p.value")
